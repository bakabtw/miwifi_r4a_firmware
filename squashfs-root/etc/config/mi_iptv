#iptv vlan tag
config iptv settings
	option internet_tag	'0'
	option internet_vid	'-1'
	option priority		'-1'
	option profile		'0'
	option lan_port1	'0'
	option lan_port2	'0'
    #wan internet vlan tag
	option data_tag		'0'
	option data_vid		'0'
	option data_priority	'0'
	option data_profile	'0'

config vlan vid
    option lan  '1'
    option wan  '2'

